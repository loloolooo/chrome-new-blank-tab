{
  "manifest_version": 2,
  "name": "Chrome New Blank Tab",
  "version": "1.0.0",

  "description": "Display a blank page when opening new tabs.",
  "homepage_url": "https://github.com/xinaghongai/chrome-new-blank-tab",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon@16.png",
    "48": "icon@48.png",
    "128": "icon@128.png"
  },
  "content_security_policy": "default-src 'none'",
  "incognito": "not_allowed",
  "externally_connectable": {
    "ids": []
  }
}